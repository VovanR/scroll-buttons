    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.0",
    "lodash": "^3.9.3",
    "mocha": "^2.2.5",
    "mocha-phantomjs": "^3.5.3",
    "phantomjs": "^1.9.17",
    "requirejs": "^2.1.18",
    "sinon": "^1.14.1"
  },
  "bugs": {
